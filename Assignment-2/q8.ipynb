    "n=int(input(\"Enter the limit \"))\n",
    "dl={}\n",
    "dl[2]=\"Prime\"\n",
    "for i in range(2,n+1):\n",
    "    for j in range(2,i):\n",
    "        if(i%j==0):\n",
    "            dl[i]='Non prime'\n",
    "            break\n",
    "        else:\n",
    "            dl[i]='prime'\n",
    "dl    \n",
